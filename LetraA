package R_Alfabeto;

public class LetraA implements Refact {
    public char[][] desenhar() {
        char[][] arrA = {
                {'A', 'A', 'A', 'A', 'A'},
                {'A', ' ', ' ', ' ', 'A'},
                {'A', 'A', 'A', 'A', 'A'},
                {'A', ' ', ' ', ' ', 'A'},
                {'A', ' ', ' ', ' ', 'A'}
        };

        Escritor escritor = new Escritor();

        return escritor.escrever(arrA);
    }

}
